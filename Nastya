				if (this.value == 'Premium') {
					$("#premiumService").prop('value', 'GO FOR PREMIUM');
				}
				else if (this.value == 'Prime') {
					$("#premiumService").prop('value', 'GO FOR PRIME TIME');
				}
				
				});
			}
		}
		/*====== CALL POP FOR PL/PT IN NORMAL CASE=======*/
		
		$('#vrfcd').on('click', function(e){
			var phone = $('#phone').val() || '';
			var appDate = '';
			var data = '';
			if(appDate!=''){
				$.ajax({
					url : 'appointment.php',
					type: 'POST',
					data: {'gofor' : 'captureauth', 'data' : data,'appDate' : appDate,'phone' : phone},
					beforeSend: function(){
						$("#overlay").show();
					},
					success: function(data){
						$("#overlay").hide();
						alert(data.trim());
						if(data.trim() == 'Request limit exceeded.Please try again next day.')
						{
						   document.location.href="book_appointment.php";
						}
					}
				});
			}
		});
		
	});
